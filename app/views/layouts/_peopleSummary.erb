<% @list.each_with_index do |person, index| %>
	<div class="col-md-4 col-lg-4 text-center">
		<% if Rails.application.assets.find_asset('people/'+person[:contact][:mobile]+'.jpg') != nil %>
	    	<img src="<%= asset_path('people/'+person[:contact][:mobile]+'.jpg') %>" class="img-circle img-responsive img-center center-block">
	    <% else %>
	    	 <img src="<%= asset_path('200x200.png') %>" class="img-circle img-responsive img-center center-block">
	    <% end %>
	    <h3><%= person[:name] %></h3>
	        <small><%= person[:position] %></small>
	</div>
<% end %>
